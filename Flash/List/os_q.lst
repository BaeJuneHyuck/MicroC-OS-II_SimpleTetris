    437              OS_ENTER_CRITICAL();
    438              switch (OSTCBCur->OSTCBStatPend) {                /* See if we timed-out or aborted                */
    439                  case OS_STAT_PEND_OK:                         /* Extract message from TCB (Put there by QPost) */
    440                       pmsg =  OSTCBCur->OSTCBMsg;
